<!DOCTYPE html>
<html>
<head>
    <title{% block title %}Welcome in Home Page{% endblock %}</title>
    {% block head_css %}

        <link type="text/css" rel="stylesheet" href="{{ asset('bundles/appblog/css/admin.css') }}">
        <link type="text/css" rel="stylesheet" href="{{ asset('bundles/appblog/css/bootstrap.css') }}">
        <link type="text/css" rel="stylesheet" href="{{ asset('bundles/appblog/css/jquery.fancybox.css') }}">
    {% endblock %}

    {% block head_js %}
        <script type="text/javascript" src="{{ asset('bundles/appblog/js/jquery-1.9.0.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/appblog/js/bootstrap.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/appblog/js/jquery.fancybox.pack.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/appblog/js/jquery.mousewheel-3.0.6.pack.js') }}"></script>
    {% endblock %}
</head>
<body>

<section>

    <div class="container">
        {% block content %}{% endblock %}
    </div>
</section>


{% block javascript %} {% endblock %}

</body>
</html>