";}s:5:"types";a:9:{i:1;i:2;i:2;i:2;i:3;i:2;i:4;i:2;i:5;i:2;i:6;i:2;i:7;i:2;i:8;i:2;i:9;i:2;}s:11:"executionMS";d:0.0029549598693847656;s:11:"explainable";b:1;}i:82;a:5:{s:3:"sql";s:8:""COMMIT"";s:6:"params";a:0:{}s:5:"types";N;s:11:"executionMS";d:0.11938905715942383;s:11:"explainable";b:1;}i:83;a:5:{s:3:"sql";s:274:"SELECT t0.id AS id1, t0.subtopic AS subtopic2, t0.title AS title3, t0.date AS date4, t0.post AS post5, t0.img_url AS img_url6, t0.author_name AS author_name7, t0.author_avatar_url AS author_avatar_url8, t0.author_category AS author_category9 FROM Contents t0 WHERE t0.id = ?";s:6:"params";a:1:{i:0;s:86:"http://www.examiner.com/article/obama-gives-ukraine-government-new-security-technology";}s:5:"types";a:1:{i:0;i:2;}s:11:"executionMS";d:0.0012059211730957031;s:11:"explainable";b:1;}i:84;a:5:{s:3:"sql";s:274:"SELECT t0.id AS id1, t0.subtopic AS subtopic2, t0.title AS title3, t0.date AS date4, t0.post AS post5, t0.img_url AS img_url6, t0.author_name AS author_name7, t0.author_avatar_url AS author_avatar_url8, t0.author_category AS author_category9 FROM Contents t0 WHERE t0.id = ?";s:6:"params";a:1:{i:0;s:90:"http://www.examiner.com/article/selfie-disaster-facebook-post-leads-to-shoplifter-s-arrest";}s:5:"types";a:1:{i:0;i:2;}s:11:"executionMS";d:0.00082182884216308594;s:11:"explainable";b:1;}i:85;a:5:{s:3:"sql";s:274:"SELECT t0.id AS id1, t0.subtopic AS subtopic2, t0.title AS title3, t0.date AS date4, t0.post AS post5, t0.img_url AS img_url6, t0.author_name AS author_name7, t0.author_avatar_url AS author_avatar_url8, t0.author_category AS author_category9 FROM Contents t0 WHERE t0.id = ?";s:6:"params";a:1:{i:0;s:109:"http://www.examiner.com/article/apple-releases-ios-8-beta-4-and-a-report-finds-ios-general-leaks-private-info";}s:5:"types";a:1:{i:0;i:2;}s:11:"executionMS";d:0.00080013275146484375;s:11:"explainable";b:1;}}}s:11:"connections";a:1:{s:7:"default";s:32:"doctrine.dbal.default_connection";}s:8:"managers";a:1:{s:7:"default";s:35:"doctrine.orm.defa